			<Add library="GLU" />
			<Add library="Xxf86vm" />
			<Add directory="$(#glut.lib)" />
		</Linker>
		<Unit filename="include/ActionListener.h" />
		<Unit filename="include/Button.h" />
		<Unit filename="include/Content.h" />
		<Unit filename="include/Frame.h" />
		<Unit filename="main.cpp" />
