		<Unit filename="src/Button.cpp" />
		<Unit filename="src/Content.cpp" />
		<Unit filename="src/Frame.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
